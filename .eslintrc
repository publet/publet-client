{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "react/jsx-key": 2,
    "react/jsx-no-duplicate-props": 1,
    "global-require": 1,
    "comma-dangle": 1,
    "eqeqeq": [2, "allow-null"],
    "no-trailing-spaces": 2,
    "no-debugger": 2
  }
}
